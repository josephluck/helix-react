{
  "name": "helix-react",
  "version": "2.0.0",
  "description": "React renderer for helix",
  "main": "lib/index.js",
  "files": [
    "src/",
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/josephluck/helix-react"
  },
  "typings": "./lib/types.d.ts",
  "scripts": {
    "dist": "rm -rf ./lib && mkdir lib && tsc && npm run browserify && npm run minify && npm run types",
    "types": "cp ./src/types.d.ts ./lib/types.d.ts",
    "browserify": "browserify -g babelify lib/index.js -o lib/bundle.js",
    "minify": "uglifyjs lib/bundle.js -c -m -o lib/bundle.min.js",
    "deploy": "npm run dist && npm publish"
  },
  "author": "Joseph Luck",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "tsify": "^3.0.1",
    "tslint": "^4.4.2",
    "tslint-config-standard": "^4.0.0",
    "typescript": "^2.2.1",
    "uglify-js": "^2.7.5"
  },
  "dependencies": {
    "@types/react": "^15.0.21",
    "helix-js": "file:../helix",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "twine-js": "file:../twine"
  }
}
